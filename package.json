{
	"name": "nodeshell",
	"version": "0.1.0",
	"description": "A shell replacement based on node",
	"main": "shell.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "node -r source-map-support/register scripts/test.js",
		"start": "node -r source-map-support/register dist/shell",
